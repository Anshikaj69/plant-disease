{
    "version":2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "functions": {
        "default": {
          "runtime": "nodejs18.x"
        }
      }
    }
